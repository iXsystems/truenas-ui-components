{
  "name": "truenas-ui",
  "version": "0.0.0",
  "main": "./dist/truenas-ui/fesm2022/truenas-ui.mjs",
  "module": "./dist/truenas-ui/fesm2022/truenas-ui.mjs",
  "typings": "./dist/truenas-ui/index.d.ts",
  "bin": {
    "truenas-icons": "./dist/truenas-ui/scripts/icon-sprite/cli.js"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/truenas-ui/index.d.ts",
      "default": "./dist/truenas-ui/fesm2022/truenas-ui.mjs"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "generate-icons": "node scripts/generate-icon-constants.js",
    "generate-sprite": "npx tsx projects/truenas-ui/scripts/icon-sprite/make-sprite.ts",
    "generate-harness-docs": "npx tsx scripts/generate-harness-docs.ts",
    "prebuild": "npm run generate-icons",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --config ./projects/truenas-ui/jest.config.ts",
    "test-cc": "yarn jest --clearCache && yarn jest --config ./projects/truenas-ui/jest.config.ts",
    "test-coverage": "yarn jest --clearCache && yarn jest --config ./projects/truenas-ui/jest.config.ts --coverage",
    "test-sb": "test-storybook",
    "lint": "eslint \"projects/truenas-ui/src/**/*.{ts,html}\" \"scripts/**/*.ts\"",
    "lint:fix": "eslint \"projects/truenas-ui/src/**/*.{ts,html}\" \"scripts/**/*.ts\" --fix",
    "copy-themes": "cp ./projects/truenas-ui/src/styles/themes.css ./projects/truenas-ui/.storybook/public/",
    "copy-sprite": "cp ./projects/truenas-ui/assets/tn-icons/sprite.svg ./projects/truenas-ui/.storybook/public/assets/tn-icons/ && cp ./projects/truenas-ui/assets/tn-icons/sprite-config.json ./projects/truenas-ui/.storybook/public/assets/tn-icons/",
    "copy-assets": "yarn copy-themes && yarn copy-sprite",
    "storybook": "yarn generate-sprite && yarn copy-assets && yarn generate-harness-docs && ng run truenas-ui:storybook",
    "build-storybook": "yarn generate-sprite && yarn copy-assets && yarn generate-harness-docs && ng run truenas-ui:build-storybook",
    "sb": "yarn copy-assets && yarn run storybook --host 0.0.0.0",
    "sbh": "NODE_OPTIONS='--max-old-space-size=4096' yarn copy-assets && yarn run storybook --host 0.0.0.0 --no-open --loglevel warn",
    "reinstall": "rm -rf ./node_modules && yarn install",
    "prepare": "husky"
  },
  "private": true,
  "peerDependencies": {
    "@angular/cdk": "^20.0.0",
    "rxjs": "^7.5.0",
    "zone.js": "^0.15.0"
  },
  "dependencies": {
    "@material-design-icons/svg": "~0.14.13",
    "@mdi/angular-material": "^7.2.96",
    "@mdi/js": "^7.4.47",
    "@mdi/svg": "~7.4.47",
    "@types/svg-sprite": "~0.0.39",
    "@types/vinyl": "~2.0.12",
    "cheerio": "~1.0.0",
    "fast-glob": "^3.3.3",
    "svg-sprite": "~2.0.4",
    "tslib": "^2.3.0",
    "tsx": "~4.19.1",
    "vinyl": "~3.0.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2101.0",
    "@angular-devkit/build-angular": "^21.1.0",
    "@angular-devkit/core": "^21.1.0",
    "@angular-eslint/builder": "^21.1.0",
    "@angular-eslint/eslint-plugin": "^21.1.0",
    "@angular-eslint/eslint-plugin-template": "^21.1.0",
    "@angular-eslint/template-parser": "^21.1.0",
    "@angular/animations": "^21.1.0",
    "@angular/cdk": "^20.2.8",
    "@angular/cli": "^21.1.0",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@nrwl/jest": "^19.8.4",
    "@storybook/addon-a11y": "^7.5.0",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-links": "^8.5.8",
    "@storybook/addon-onboarding": "^8.5.8",
    "@storybook/addon-themes": "^8.6.4",
    "@storybook/addons": "^7.6.17",
    "@storybook/angular": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/jest": "^0.2.3",
    "@storybook/test": "^8.5.8",
    "@storybook/test-runner": "^0.22.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@typescript-eslint/types": "^8.51.0",
    "@typescript-eslint/utils": "^8.51.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "jasmine-core": "~4.3.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "14.2.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lucide": "^0.545.0",
    "ng-packagr": "^21.1.0",
    "rxjs": "^7.8.2",
    "storybook": "^8.5.8",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.3.0",
    "typescript": "~5.9.3",
    "zone.js": "^0.15.1"
  },
  "packageManager": "yarn@4.10.3"
}
