{
  "name": "truenas-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --config ./projects/truenas-ui/jest.config.ts",
    "test-cc": "yarn jest --clearCache && yarn jest --config ./projects/truenas-ui/jest.config.ts",
    "test-coverage": "yarn jest --clearCache && yarn jest --config ./projects/truenas-ui/jest.config.ts --coverage",
    "test-sb": "test-storybook",
    "copy-themes": "cp ./projects/truenas-ui/src/styles/themes.css ./projects/truenas-ui/.storybook/public/",
    "storybook": "yarn copy-themes && ng run truenas-ui:storybook",
    "build-storybook": "yarn copy-themes && ng run truenas-ui:build-storybook",
    "sb": "yarn copy-themes && yarn run storybook --host 0.0.0.0",
    "sbh": "yarn copy-themes && yarn run storybook --host 0.0.0.0 --no-open",
    "reinstall": "rm -rf ./node_modules && yarn install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@mdi/angular-material": "^7.2.96",
    "@mdi/js": "^7.4.47",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1902.0",
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular-devkit/core": "^19.2.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@nrwl/jest": "^19.8.4",
    "@storybook/addon-a11y": "^7.5.0",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-links": "^8.5.8",
    "@storybook/addon-onboarding": "^8.5.8",
    "@storybook/addon-themes": "^8.6.4",
    "@storybook/addons": "^7.6.17",
    "@storybook/angular": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/jest": "^0.2.3",
    "@storybook/test": "^8.5.8",
    "@storybook/test-runner": "^0.22.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^29.5.14",
    "css-loader": "^7.1.2",
    "jasmine-core": "~4.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "14.2.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lucide": "^0.545.0",
    "ng-packagr": "^19.0.0",
    "storybook": "^8.5.8",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.3.0",
    "typescript": "~5.6.0",
    "zone.js": "^0.14.2"
  },
  "packageManager": "yarn@4.10.3"
}
