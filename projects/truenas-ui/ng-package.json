{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/truenas-ui",
  "assets": [
    "src/styles/*.css",
    "src/assets/**/*",
    "assets/**/*",
    "scripts/**/*"
  ],
  "lib": {
    "entryFile": "src/public-api.ts"
  },
  "allowedNonPeerDependencies": [
    "@mdi/svg",
    "@material-design-icons/svg",
    "cheerio",
    "svg-sprite",
    "vinyl",
    "tsx",
    "@types/svg-sprite",
    "@types/vinyl"
  ]
}